{
  "name": "middle.messenger.praktikum.yandex",
  "browserslist": [
    "> 0.5%",
    "last 3 versions",
    "not dead"
  ],
  "engines": {
    "node": ">= 16.14.2"
  },
  "parcelCleanPaths": [
    "dist",
    ".cache",
    ".parcel-cache"
  ],
  "version": "1.0.0",
  "description": "---",
  "scripts": {
    "serve": "webpack serve --mode=development --progress --config webpack/webpack.dev.js",
    "test": "jest",
    "eslint": "npx eslint \"./src/**/*.ts\" --ext .ts",
    "eslint:fix": "eslint --fix \"**/*.ts\"",
    "csslint": "npx stylelint \"./src/**/*.scss\"",
    "lint": "npm run eslint && npm run csslint",
    "prettier": "prettier --check \"**/*.{ts,css,html,md,json}\" --write",
    "build": "npm run clean && webpack --progress --mode=production --config webpack/webpack.prod.js",
    "start": "npm run build && node server.js",
    "clean": "rimraf build",
    "validate": "npm audit && npm-run-all --parallel test eslint csslint",
    "huskysetup": "npx husky install && npx husky add .husky/pre-commit \"npm run validate\"",
    "prepare": "husky install"
  },
  "alias": {
    "core": "./src/core",
    "components": "./src/components",
    "pages": "./src/pages",
    "data": "./src/data",
    "images": "./src/asserts/images"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dormeh/middle.messenger.praktikum.yandex.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dormeh/middle.messenger.praktikum.yandex/issues"
  },
  "homepage": "https://github.com/Dormeh/middle.messenger.praktikum.yandex#readme",
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@testing-library/dom": "8.20.0",
    "@testing-library/jest-dom": "5.16.5",
    "@types/jest": "29.4.0",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "babel-loader": "9.1.2",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.3",
    "dotenv": "16.0.3",
    "eslint": "8.22.0",
    "file-loader": "6.2.0",
    "handlebars-loader": "1.7.3",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.4.2",
    "jest-environment-jsdom": "29.4.2",
    "mini-css-extract-plugin": "2.7.2",
    "msw": "1.0.0",
    "prettier": "2.8.1",
    "rimraf": "4.1.2",
    "sass-loader": "13.2.0",
    "style-loader": "3.3.1",
    "stylelint": "14.16.0",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "6.1.0",
    "terser-webpack-plugin": "5.3.6",
    "ts-loader": "9.4.2",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "typescript": "4.9.4",
    "webpack": "5.75.0",
    "webpack-bundle-analyzer": "4.7.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1",
    "webpack-merge": "5.8.0",
    "webpack-node-externals": "3.0.0",
    "whatwg-fetch": "3.6.2"
  },
  "dependencies": {
    "express": "4.18.2",
    "handlebars": "4.7.7",
    "nanoid": "3.3.1",
    "sass": "^1.58.0"
  }
}
